#
# Just import all neccesary files in ~/.config/zsh
#

# import zsh-aliases.zsh
source ~/.config/zsh/zsh-aliases.zsh
# import zsh-variables.zsh
source ~/.config/zsh/zsh-variables.zsh
# import zsh-plugins
source ~/.config/zsh/plugins/zsh-sudo/sudo.zsh
source ~/.config/zsh/plugins/zsh-autosuggestions/zsh-autosuggestions.zsh
source ~/.config/zsh/plugins/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
